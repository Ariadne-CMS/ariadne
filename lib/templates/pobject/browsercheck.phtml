<?php
  global $HTTP_USER_AGENT;
  if (eregi(".*msie ([0-9]+).*",$HTTP_USER_AGENT, $regs)) && 
       $regs[1]) {
    $arResult="msie".(intval($regs[1]));
  } else if (eregi(".*Mozilla/([0-9]+).*, $HTTP_USER_AGENT, $regs) &&
       $regs[1]) {
    $arResult="ns".(intval($regs[1]));
  } else {
    $arResult="other";
  }
?>