<?php
  // template not found, try ancestor
  $arCallTemplate=$this->store->code."templates/pobject/".$arCallFunction;
  if (file_exists($arCallTemplate)) {
    include($arCallTemplate);
  } else if ($this->exists($this->path.$arCallFunction."/")) {
    $this->get($this->path.$arCallFunction."/");
  } else {
    include($this->store->code."templates/pobject/default.phtml");
  }
?>